## create collection

{ "@context": "https://app.alveo.edu.au/schema/json-ld",
  "alveo:metadata": {
         "@type": "dcmitype:Collection",   #  remove == redundant
         "dc:identifier" : "mycollection",  ## == 'name'
         "dc:creator": "Data Owner",
         "dc:rights": "All rights reserved to Data Owner",
         "dc:subject": "English Language",
         "dc:title": "Test Collection"
     }
}


# document

# by URI
{ "@context": "https://app.alveo.edu.au/schema/json-ld",
  "alveo:metadata": {
        "dc:source": "https://app.alveo.edu.au/catalog/ace/J68/document/J68-plain.txt",
        "dc:type": "Text",
        "alveo:size": null,  ## what's this?
        "rdf:type": "http://xmlns.com/foaf/0.1/Document",  # remove == redundant
        "dc:identifier": "J68-plain.txt",
        "dc:title": "J68#Text",
        "dc:extent": "11911"   ## calculate
        }
}

